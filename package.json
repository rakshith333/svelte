{
	"name": "sveltekit-template",
	"description": "sveltekit + typescript + tailwindcss + cloudflare",
	"version": "1.0.0",
	"author": {
		"name": "Rohan Babu A M",
		"url": "https://renderpub.com",
		"email": "rohanbabu.am@renderpub.com"
	},
	"license": "MIT",
	"keywords": [
		"sveltekit",
		"typescript",
		"svelte",
		"SSR",
		"svelte-starter",
		"sveltekit-starter",
		"svelte-boilerplate",
		"sveltekit-boilerplate",
		"svelte boilerplate",
		"sveltekit boilerplate",
		"svelte-tailwind-boilerplate",
		"svelte tailwind boilerplate",
		"sveltekit tailwind boilerplate",
		"svelte-ssr-starter",
		"sveltekit-ssr-starter",
		"svelte-pwa-starter",
		"sveltekit-pwa-starter",
		"starter-template",
		"svelte ssr starter",
		"sveltekit ssr starter",
		"svelte pwa starter",
		"sveltekit pwa starter",
		"starter template",
		"template"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/navneetsharmaui/sveltekit-starter.git"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky install",
		"prebuild": "rimraf .svelte-kit && rimraf build",
		"dev:sveltekit": "vite dev ",
		"dev:sveltekit:host": "vite dev --host=0.0.0.0",
		"dev": "pnpm dev:sveltekit",
		"dev:host": "pnpm dev:sveltekit:host",
		"dev:open": "pnpm dev --open",
		"dev:host:open": "pnpm dev --open --host",
		"build:sveltekit": "cross-env-shell vite build",
		"build": "run-s build:sveltekit",
		"build:verbose:sveltekit": "cross-env-shell vite build --verbose",
		"build:verbose": "run-s build:verbose:sveltekit",
		"build:cloudflare": "cross-env ADAPTER=cloudflare OPTIONS='{}' pnpm build",
		"build:cloudflare:verbose": "cross-env ADAPTER=cloudflare OPTIONS='{}' pnpm build:verbose",
		"build:cloudflare:ssr": "cross-env ADAPTER=cloudflare OPTIONS='{}' SSR=true pnpm build",
		"build:cloudflare:verbose:ssr": "cross-env ADAPTER=cloudflare OPTIONS='{}' SSR=true pnpm build:verbose",
		"preview": "vite preview",
		"preview:open": "pnpm run preview --open",
		"preview:host": "pnpm run preview --host",
		"preview:host:open": "pnpm run preview --open --host",
		"lint": "prettier --ignore-path .gitignore --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} --plugin-search-dir=. && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore",
		"lint:fix": "prettier --check ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} --plugin-search-dir=. && eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore",
		"format": "prettier --ignore-path .gitignore --write ./src/**/*.{js,svelte,ts} ./*.{ts,cjs,js} --plugin-search-dir=.",
		"eslint": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore",
		"eslint:fix": "eslint ./src/**/*.{js,svelte,ts} --ignore-path .gitignore .eslintignore --fix",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "1.0.0-next.31",
		"@sveltejs/kit": "1.0.0-next.443",
		"@tailwindcss/typography": "^0.5.4",
		"@types/node": "^18.7.8",
		"@types/node-fetch": "^2.5.12",
		"@types/nprogress": "^0.2.0",
		"@typescript-eslint/eslint-plugin": "^5.33.1",
		"@typescript-eslint/parser": "^5.33.1",
		"autoprefixer": "^10.4.8",
		"cross-env": "^7.0.3",
		"cssnano": "^5.1.3",
		"cssnano-preset-advanced": "^5.3.8",
		"eslint": "^8.22.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.4.2",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"node-fetch": "^3.2.0",
		"npm-run-all": "^4.1.5",
		"nprogress": "^0.2.0",
		"postcss": "^8.4.16",
		"postcss-cli": "^10.0.0",
		"postcss-load-config": "^4.0.1",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.9"
	},
	"type": "module",
	"engines": {
		"node": ">= 17.9.1"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.2",
		"@fortawesome/free-solid-svg-icons": "^6.1.2",
		"node": "^19.0.0",
		"pnpm": "^7.12.2",
		"sveltekit-form": "^0.1.0",
		"tween": "^0.9.0"
	}
}
